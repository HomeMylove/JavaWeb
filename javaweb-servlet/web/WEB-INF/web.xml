<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>ServletDemo</servlet-name>
        <servlet-class>com.neta.servlet.ServletDemo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletDemo</servlet-name>
        <url-pattern>/demo</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ServletGetAttr</servlet-name>
        <servlet-class>com.neta.servlet.ServletGetAttr</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletGetAttr</servlet-name>
        <url-pattern>/get</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletSetAttr</servlet-name>
        <servlet-class>com.neta.servlet.ServletSetAttr</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletSetAttr</servlet-name>
        <url-pattern>/set</url-pattern>
    </servlet-mapping>
</web-app>